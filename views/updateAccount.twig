{% extends "base.twig" %}

{% block title %} Update Account {% endblock %}

{% block header %}Update Account{%endblock%}

{% block content %}

{% if notification %}
  <h1>{{ notification }}</h1>
{% endif %}

<form class="updateAccountForm" method="POST" enctype="multipart/form-data">
  {{ form_errors(form) }}
  {{ form_widget(form) }}
  {{ form_rest(form) }}
</form>
<form class="cancel_button">
  <input onclick="location.href='/'" type="button" value="Cancel"/>
</form>

{% endblock %}
